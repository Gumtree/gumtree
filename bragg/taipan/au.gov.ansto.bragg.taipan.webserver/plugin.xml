<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="taipanHttpcontext"
         point="org.eclipse.equinox.http.registry.httpcontexts">
      <httpcontext
            id="taipan">
         <resource-mapping
               path="/taipan/rest">
         </resource-mapping>
      </httpcontext>
   </extension>
   <extension
         id="taipanRestlet"
         point="org.eclipse.equinox.http.registry.servlets">
      <servlet
            alias="/taipan/rest"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="taipan">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.taipan.webserver.restlet.TaipanRestletApplication">
         </init-param>
      </servlet>
   </extension>
   <extension
         point="org.eclipse.equinox.http.registry.resources">
      <resource
            alias="/taipan.html"
            base-name="/WebContent/taipan.html">
      </resource>
      <resource
            alias="/index.html"
            base-name="/WebContent/index.html">
      </resource>
      <resource
            alias="/taipan.swf"
            base-name="/WebContent/taipan.swf">
      </resource>
      <resource
            alias="/playerProductInstall.swf"
            base-name="/WebContent/playerProductInstall.swf">
      </resource>
      <resource
            alias="/AC_OETags.js"
            base-name="/WebContent/AC_OETags.js">
      </resource>
      <resource
            alias="/history/history.css"
            base-name="/WebContent/history/history.css">
      </resource>
      <resource
            alias="/history/history.js"
            base-name="/WebContent/history/history.js">
      </resource>
      <resource
            alias="/history/historyFrame.html"
            base-name="/WebContent/history/historyFrame.html">
      </resource>
      <resource
            alias="/mobile.html"
            base-name="/WebContent/mobile.html">
      </resource>
      <resource
            alias="/mobile.js"
            base-name="/WebContent/mobile.js">
      </resource>
      <resource
            alias="/mobile.css"
            base-name="/WebContent/mobile.css">
      </resource>
      <resource
            alias="/instrumentsetup.js"
            base-name="/WebContent/instrumentsetup.js">
      </resource>
      <resource
            alias="/mobilehistmem.html"
            base-name="/WebContent/mobilehistmem.html">
      </resource>
      <resource
            alias="/mobilehistmem.js"
            base-name="/WebContent/mobilehistmem.js">
      </resource>
               
   </extension>
</plugin>
