<plugin>
   <extension
         point="org.gumtree.ui.cli.interpreter">
      <interpreterClass
            class="au.gov.ansto.bragg.quokka.exp.core.interpreter.QuokkaInterpreter"
            name="quokka.exp">
      </interpreterClass>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="au.gov.ansto.bragg.quokka.exp.ui.ExperimentPlotEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="xml"
            icon="icons/experiment_plot.gif"
            id="au.gov.ansto.bragg.quokka.exp.ui.ExperimentPlotEditor"
            name="Experiment Plots">
      </editor>
   </extension>

</plugin>
