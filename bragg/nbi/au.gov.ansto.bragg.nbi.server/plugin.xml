            alias="/user/jython/user"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="userManagerRestlet">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.restlet.UserManagerApplication">
         </init-param>
      </servlet>
      <servlet
            alias="/doc/notebook"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="notebookDocRestlet">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.notebook.NotebookApplication">
         </init-param>
      </servlet>
      <servlet
            alias="/user/notebook"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="notebookAdminRestlet">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.notebook.NotebookApplication">
         </init-param>
      </servlet>
      <servlet
            alias="/doc/catalog"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="catalogDocRestlet">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.catalog.CatalogApplication">
         </init-param>
      </servlet>
      <servlet
            alias="/user/catalog"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="catalogAdminRestlet">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.catalog.CatalogApplication">
         </init-param>
      </servlet>
      <servlet
            alias="/user/yaml"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="yamlRestlet">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.yaml.YamlApplication">
         </init-param>
      </servlet>
      <servlet
            alias="/doc/db"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="databaseDocRestlet">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.notebook.DatabaseApplication">
         </init-param>
      </servlet>
      <servlet
            alias="/user/db"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="databaseAdminRestlet">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.notebook.DatabaseApplication">
         </init-param>
      </servlet>
      <servlet
            alias="/user/signin"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="signinRestlet">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.login.SigninApplication">
         </init-param>
      </servlet>
      <servlet
            alias="/user/home"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="userHomeRestlet">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.login.UserHomeApplication">
