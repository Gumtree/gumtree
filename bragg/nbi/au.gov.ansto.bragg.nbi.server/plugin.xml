            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="NBRedirectRestlet">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.notebook.NBManagerRedirectApplication">
         </init-param>
      </servlet>
      <servlet
            alias="/status/admin/notebookAdmin.html"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="NBAdminRedirectRestlet">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.notebook.NBAdminRedirectApplication">
         </init-param>
      </servlet>
   </extension>

</plugin>
