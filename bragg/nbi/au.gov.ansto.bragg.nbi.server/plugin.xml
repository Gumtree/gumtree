<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="nsHttpcontext"
         point="org.eclipse.equinox.http.registry.httpcontexts">
      <httpcontext
            id="ns">
         <resource-mapping
               path="/status/ns/rest">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="statisticRestlet">
         <resource-mapping
               path="/status/st/rest">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="manualsRestlet">
         <resource-mapping
               path="/status/manuals">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="jythonRestlet">
         <resource-mapping
               path="/user/jython/rest">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="jythonRunnerRestlet">
         <resource-mapping
               path="/user/jython/runner">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="userManagerRestlet">
         <resource-mapping
               path="/user/jython/user">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="notebookDocRestlet">
         <resource-mapping
               path="/doc/notebook">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="notebookAdminRestlet">
         <resource-mapping
               path="/user/notebook">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="catalogDocRestlet">
         <resource-mapping
               path="/doc/catalog">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="catalogAdminRestlet">
         <resource-mapping
               path="/user/catalog">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="yamlRestlet">
         <resource-mapping
               path="/user/yaml">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="NBPageRedirectRestlet">
         <resource-mapping
               path="/status/notebook.html">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="NBAdminRedirectRestlet">
         <resource-mapping
               path="/status/admin/notebookAdmin.html">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="NBRedirectRestlet">
         <resource-mapping
               path="/status/notebookAdmin.html">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="databaseDocRestlet">
         <resource-mapping
               path="/doc/db">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="databaseAdminRestlet">
         <resource-mapping
               path="/user/db">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="signinRestlet">
         <resource-mapping
               path="/user/signin">
         </resource-mapping>
      </httpcontext>
      <httpcontext
            id="userHomeRestlet">
         <resource-mapping
               path="/user/home">
         </resource-mapping>
      </httpcontext>
   </extension>
   <extension
         id="nsRestlet"
         point="org.eclipse.equinox.http.registry.servlets">
      <servlet
            alias="/status/ns/rest"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="ns">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.restlet.NSRestletApplication">
         </init-param>
      </servlet>
      <servlet
            alias="/status/manuals"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="manualsRestlet">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.restlet.ManualsApplication">
         </init-param>
      </servlet>
      <servlet
            alias="/status/st/rest"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="statisticRestlet">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.restlet.StatisticRestletApplication">
         </init-param>
      </servlet>
      <servlet
            alias="/user/jython/rest"
            class="org.restlet.ext.servlet.ServerServlet"
            httpcontextId="jythonRestlet">
         <init-param
               name="org.restlet.application"
               value="au.gov.ansto.bragg.nbi.server.restlet.JythonRestletApplication">
